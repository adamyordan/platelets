def clear_screen():
    if platform.system().lower() == 'windows':
        os.system('cls')
    else:
        os.system('clear')

if __name__ == '__main__':
    while True:
        clear_screen()
        print('\n' + art1)
        if input() == 'q':
            exit(0)
        clear_screen()
        print('\n' + art2)
        time.sleep(0.1)
